<!--pages/list/list.wxml-->
<view class='data-item' wx:for="{{weekWeather}}">
      <view>
            <view class='weenkend'>{{item.day}}</view>
            <view class='date-today'>{{item.date}}</view>
      </view>
      <view class='temp'>{{item.temp}}</view>
      <image class='img' src='{{item.iconPath}}'></image>
</view>
